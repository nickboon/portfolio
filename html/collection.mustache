<div class="column indented">
	<{{htmlHeader}}>
		<span class="closed" id="{{& id}}">{{start}} {{title}} </span>
	</{{htmlHeader}}>
	<div class="{{& id}}_content indented js_closed">
		{{>images}}
		{{#info}}	
		<{{htmlSubheader}}>
			<span class="closed" id="{{& id}}_info">{{start}} Info </span>
		</{{htmlSubheader}}>
		<div class="{{& id}}_info_content js_closed">
			<div class="indented">{{{info}}}</div>
			<{{htmlSubheader}}>{{end}}</{{htmlSubheader}}>
		</div>				
		{{/info}}
		<div class="{{linksClass}}">
			<{{htmlSubheader}}>
				<span class="closed" id="{{& id}}_links">{{start}} Links </span>
			</{{htmlSubheader}}>
			<div class="{{& id}}_links_content js_closed">
				<div class="indented">
					{{#links}}	
					<a href="{{& .}}" class="clickable">{{& .}}</a>
					{{/links}}
				</div>
				<{{htmlSubheader}}>{{end}}</{{htmlSubheader}}>
			</div>				
		</div>
	</div>
</div>

<div class="indented">
{{#subcollections}}{{>collection}}{{/subcollections}}
</div>

<{{htmlHeader}} class="{{& id}}_content column indented js_closed">{{end}}</{{htmlHeader}}>
