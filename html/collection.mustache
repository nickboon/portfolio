<div class="column indented">
	{{#htmlHeader}}<{{htmlHeader}}>{{/htmlHeader}}
		<span class="closed" {{#cssIdentifier}}id="{{& cssIdentifier}}"{{/cssIdentifier}}>{{start}} {{title}} </span>
	{{#htmlHeader}}</{{htmlHeader}}>{{/htmlHeader}}
	<div class="{{& cssIdentifier}}_content indented js_closed">
		{{>images}}
		{{#info}}	
		{{#htmlSubheader}}<{{htmlSubheader}}>{{/htmlSubheader}}
			<span class="closed" {{#cssIdentifier}}id="{{& cssIdentifier}}_info"{{/cssIdentifier}}>
				{{start}} Info 
			</span>
		{{#htmlSubheader}}</{{htmlSubheader}}>{{/htmlSubheader}}
		<div class="{{& cssIdentifier}}_info_content js_closed">
			<div class="indented">{{{info}}}</div>
			{{#htmlSubheader}}<{{htmlSubheader}}>{{/htmlSubheader}}
					{{end}}
			{{#htmlSubheader}}</{{htmlSubheader}}>{{/htmlSubheader}}
		</div>				
		{{/info}}
		<div class="{{linksClass}}">
			{{#htmlSubheader}}<{{htmlSubheader}}>{{/htmlSubheader}}
				<span class="closed" {{#cssIdentifier}}id="{{& cssIdentifier}}_links"{{/cssIdentifier}}>
					{{start}} Links 
				</span>
			{{#htmlSubheader}}</{{htmlSubheader}}>{{/htmlSubheader}}
			<div class="{{& cssIdentifier}}_links_content js_closed">
				<div class="indented">
					{{#links}}	
					<a href="{{& .}}" class="clickable">{{& .}}</a>
					{{/links}}
				</div>
				{{#htmlSubheader}}<{{htmlSubheader}}>{{/htmlSubheader}}
						{{end}}
				{{#htmlSubheader}}</{{htmlSubheader}}>{{/htmlSubheader}}
			</div>				
		</div>
	</div>
</div>

<div class="indented">
{{#subcollections}}{{>collection}}{{/subcollections}}
</div>

{{#htmlHeader}}<{{htmlHeader}} class="{{& cssIdentifier}}_content column indented js_closed">{{/htmlHeader}}
	{{end}}
{{#htmlHeader}}</{{htmlHeader}}>{{/htmlHeader}}
